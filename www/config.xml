<?xml version="1.0" encoding="UTF-8"?>

<!-- config.xml reference: https://build.phonegap.com/docs/config-xml -->
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.phonegap.hello-world"
        version   = "1.0.0">

  <name>Assets Track</name>

  <description>
    Assets Track app.
  </description>

  <author href="http://www.star-knowledge.com/" email="harisha@star-knowledge.org">
    Star K Team
  </author>

  <preference name="phonegap-version" value="3.0.0" />
  <icon src="icon.png" />
  <gap:plugin name="org.apache.cordova.core.camera" />
  <gap:plugin name="com.phonegap.plugins.barcodescanner"/>
  <gap:plugin name="it.mobimentum.phonegapspinnerplugin" />
  <gap:plugin name="org.apache.cordova.dialogs" />
  <access origin="*" subdomains="true"/>
  <access uri="*" subdomains="true" />
</widget>
